# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your values

aws_region = "us-east-1"
environment = "dev"
project_name = "skillspark"

# Required: Resend API key for email notifications
resend_api_key = "your-resend-api-key-here"

# Optional: Expo access token for push notifications
expo_access_token = ""

# Path to Lambda function source code
lambda_source_path = "../lambda"

# Optional: Override default Lambda settings
# lambda_runtime = "python3.11"
# lambda_timeout = 30
# lambda_memory_size = 256
# lambda_reserved_concurrency = 2

# Optional: Override default SQS settings
# sqs_batch_size = 10
# sqs_batch_window = 5
# sqs_max_receive_count = 3

